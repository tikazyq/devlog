{
  "storage": {
    "type": "github",
    "github": {
      "owner": "your-org",
      "repo": "your-repo",
      "token": "ghp_your_github_token",
      "labelsPrefix": "devlog",
      "mapping": {
        "useNativeType": true,
        "useNativeLabels": true,
        "useStateReason": true,
        "projectId": null,
        "projectFieldMappings": {
          "priority": "Priority",
          "status": "Status",
          "type": "Type"
        }
      },
      "rateLimit": {
        "requestsPerHour": 5000,
        "retryDelay": 1000,
        "maxRetries": 3
      },
      "cache": {
        "enabled": true,
        "ttl": 300000
      }
    }
  }
}
