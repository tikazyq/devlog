{
  "storage": {
    "type": "github",
    "github": {
      "owner": "myorg",
      "repo": "my-project",
      "token": "${GITHUB_TOKEN}",
      "labelsPrefix": "devlog",
      "rateLimit": {
        "requestsPerHour": 4000,
        "retryDelay": 1000,
        "maxRetries": 3
      },
      "cache": {
        "enabled": true,
        "ttl": 300000
      }
    }
  }
}
