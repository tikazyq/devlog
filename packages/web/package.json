{
  "name": "@devlog/web",
  "version": "1.0.0",
  "description": "Web interface for devlog management",
  "main": "./next.config.js",
  "scripts": {
    "dev": "next dev",
    "dev:full": "concurrently --names \"TYPES,CORE,WEB\" --prefix-colors \"cyan,green,blue\" \"pnpm --filter @devlog/types dev\" \"pnpm --filter @devlog/core dev\" \"pnpm dev\"",
    "dev:server": "next dev --port 3000",
    "dev:client": "next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "preview": "next start",
    "clean": "rimraf .next out *.tsbuildinfo"
  },
  "dependencies": {
    "@devlog/core": "workspace:*",
    "@devlog/types": "workspace:*",
    "highlight.js": "11.11.1",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "10.1.0",
    "rehype-highlight": "7.0.2",
    "rehype-sanitize": "6.0.0",
    "remark-gfm": "4.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.2.6",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/ws": "^8.5.10",
    "antd": "^5.12.8",
    "autoprefixer": "^10.4.17",
    "concurrently": "9.2.0",
    "date-fns": "^3.2.0",
    "lucide-react": "^0.323.0",
    "postcss": "^8.4.33",
    "recharts": "^2.10.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "devlog",
    "development",
    "tracking",
    "web",
    "dashboard"
  ],
  "author": "",
  "license": "MIT"
}
